package primer_proyecto;

public class TicTac {

	public static void main(String[] args) {
			
			char [] [] gamerboard= 
				{{' ','|',' ','|',' '},
				{'-','+','-','+','-'},
				{' ','|',' ','|',' '},
				{'-','+','-','+','-'},
				{' ','|',' ','|',' '}};
			printGamerboard(gamerboard);

	}
		public static void printGamerboard(char [] [] gamerboard) {
			for(char[] row: gamerboard) {
				for(char c : row) {
					System.out.print(c);
				}
				System.out.println();
			}
		}
}
